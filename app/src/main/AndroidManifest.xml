        android:supportsRtl="true"
        android:theme="@style/Theme.Maskoki">
        <activity android:name=".layout.DetailResepi"></activity>
        <activity android:name=".layout.MainActivity" />
        <activity android:name=".layout.Registrasi" />
        <activity android:name=".layout.Login" />
        <activity android:name=".layout.SpalshScreen">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>